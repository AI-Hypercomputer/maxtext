{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": false,
      "python": "python3",
      "program": "${file}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug gpt_forward_pass",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/MaxText/gpt_check2.py",
      "args": [
        "MaxText/configs/base.yml",
        "tokenizer_type=huggingface",
        "tokenizer_path=openai/gpt-oss-20b",
        "load_parameters_path=/home/ranran_google_com/gpt_20b/maxtext/items",
        "run_name=forward_pass_test_gpt",
        "model_name=gpt-oss-20b",
        "per_device_batch_size=1",
        "max_prefill_predict_length=4",
        "max_target_length=4",
        "dataset_type=synthetic",
        "scan_layers=false",
        "sparse_matmul=True",
        "dtype=float32",
        "activations_in_float32=true",
        "matmul_precision=high",
        // "attention=dot_product",
      ],
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
